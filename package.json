{
  "name": "nestjs-starter",
  "version": "0.1.4",
  "description": "Get started for beginners with Nestjs",
  "keywords": [
    "nest",
    "nest starter",
    "nestjs starter",
    "nestjs-starter",
    "typescript nest"
  ],
  "author": "TaiPham (https://github.com/pktai)",
  "license": "MIT",
  "scripts": {
    "check-types": "tsc",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,scripts,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "pre-commit-lint": "yarn check-types && yarn lint-staged && yarn lint",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "heroku-postbuild": "yarn build"
  },
  "dependencies": {
    "@nestjs/common": "~9.0.0",
    "@nestjs/config": "~0.6.3",
    "@nestjs/core": "~7.6.18",
    "@nestjs/jwt": "~7.2.0",
    "@nestjs/mongoose": "^8.0.1",
    "@nestjs/passport": "~7.1.5",
    "@nestjs/platform-express": "~7.6.18",
    "@nestjs/platform-fastify": "~7.6.18",
    "@nestjs/platform-socket.io": "~8.0.8",
    "@nestjs/sequelize": "~0.2.0",
    "@nestjs/swagger": "~4.8.0",
    "@nestjs/websockets": "~8.0.8",
    "@types/bluebird": "~3.5.35",
    "@types/cls-hooked": "~4.3.3",
    "@types/validator": "~13.1.4",
    "class-transformer": "~0.4.0",
    "class-validator": "~0.13.1",
    "cls-hooked": "~4.2.2",
    "cookie-parser": "~1.4.5",
    "dotenv": "~10.0.0",
    "fastify-helmet": "~5.3.1",
    "fastify-rate-limit": "~5.5.0",
    "fastify-swagger": "~4.7.0",
    "lodash": "~4.17.21",
    "mongoose": "^6.0.3",
    "passport": "~0.4.1",
    "passport-jwt": "~4.0.0",
    "pg": "~8.6.0",
    "reflect-metadata": "~0.1.13",
    "sequelize": "~6.6.2",
    "sequelize-typescript": "~2.1.0",
    "socket.io": "~4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~12.1.4",
    "@commitlint/config-conventional": "~12.1.4",
    "@nestjs/cli": "~7.6.0",
    "@nestjs/schematics": "~7.3.1",
    "@nestjs/testing": "~7.6.18",
    "@types/cookie-parser": "~1.4.2",
    "@types/express": "~4.17.12",
    "@types/jest": "~26.0.23",
    "@types/node": "~15.12.2",
    "@types/passport-jwt": "~3.0.5",
    "@types/sequelize": "~4.28.9",
    "@types/supertest": "~2.0.11",
    "@typescript-eslint/eslint-plugin": "~4.27.0",
    "@typescript-eslint/parser": "~4.27.0",
    "axios": "~0.21.1",
    "commitlint": "~12.1.4",
    "cross-env": "~7.0.3",
    "eslint": "~7.28.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-prettier": "~3.4.0",
    "husky": "~6.0.0",
    "jest": "~27.0.4",
    "lint-staged": "~11.0.0",
    "prettier": "~2.3.1",
    "rimraf": "~3.0.2",
    "sequelize-cli": "~6.2.0",
    "superagent": "~6.1.0",
    "supertest": "~6.1.3",
    "ts-jest": "~27.0.3",
    "ts-loader": "~9.2.3",
    "ts-node": "~10.0.0",
    "tsconfig-paths": "~3.9.0",
    "typescript": "~4.3.4"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testMatch": ["**/tests/**/*.e2e-spec.ts"],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{js,ts}": ["cross-env NODE_ENV=development eslint --cache"],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pktai/nestjs-starter.git"
  },
  "bugs": {
    "url": "https://github.com/pktai/nestjs-starter/issues"
  },
  "contributors": ["TaiPham (https://github.com/pktai)"]
}
